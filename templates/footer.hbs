            <ul class="social">
               {{#isnt settings.facebook_url '-'}}
                  <li><a href="{{settings.facebook_url}}" target="_blank"><i class="fa-brands fa-facebook-f"></i></a></li>
               {{/isnt}}
               {{#isnt settings.twitter_url '-'}}
                  <li><a href="{{settings.twitter_url}}" target="_blank"><i class="fa-brands fa-x-twitter"></i></a></li>
               {{/isnt}}
               {{#isnt settings.linkedin_url '-'}}
                  <li><a href="{{settings.linkedin_url}}" target="_blank"><i class="fa-brands fa-linkedin-in"></i></a></li>
               {{/isnt}}
               {{#isnt settings.pinterest_url '-'}}
                  <li><a href="{{settings.pinterest_url}}" target="_blank"><i class="fa-brands fa-pinterest-p"></i></a></li>
               {{/isnt}}
               {{#isnt settings.instagram_url '-'}}
